    #endregion

    private byte[] KeyArray;
    private byte[] ToEncryptArray;
    private byte[] ResultArray;
    private ICryptoTransform CTransform;
    private MD5CryptoServiceProvider Provider;
    private TripleDESCryptoServiceProvider TDesProvider;

    private string itoa64 = "./0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz";
    
    /// <summary>
    /// This method is called once for every row that passes through the component from Input0.
    ///
    /// Example of reading a value from a column in the the row:
    ///  string zipCode = Row.ZipCode
    ///
    /// Example of writing a value to a column in the row:
    ///  Row.ZipCode = zipCode
    /// </summary>
    /// <param name="Row">The row that is currently passing through the component</param>
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        if (!Row.SourcePasswordHash_IsNull && Row.SourcePasswordHash.Length > 0)
        {
            var encryptedPassword = Encoding.Unicode.GetString(Row.SourcePasswordHash.GetBlobData(0, Convert.ToInt32(Row.SourcePasswordHash.Length)));
            var decryptedSqlPassword = this.DecryptSqlPassword(encryptedPassword, this.Variables.EncryptionKey.ToString());

            var passwordMatch = this.WpCheckPassword(decryptedSqlPassword, Row.destinationuserpass);

            Row.PasswordMatch = passwordMatch;
            Row.EncryptedPassword = passwordMatch ? Row.destinationuserpass : this.EncryptWordpressPassword(decryptedSqlPassword);
        }
    }

    private string DecryptSqlPassword(string input, string key)
    {
        ToEncryptArray = Convert.FromBase64String(input);

        Provider = new MD5CryptoServiceProvider();
        KeyArray = Provider.ComputeHash(UTF8Encoding.UTF8.GetBytes(key));
        Provider.Clear();

        TDesProvider = new TripleDESCryptoServiceProvider
        {
            Key = KeyArray,
            Mode = CipherMode.ECB,
            Padding = PaddingMode.PKCS7
        };

        CTransform = TDesProvider.CreateDecryptor();
        ResultArray = CTransform.TransformFinalBlock(ToEncryptArray, 0, ToEncryptArray.Length);
        TDesProvider.Clear();

        return UTF8Encoding.UTF8.GetString(ResultArray);
    }

    private string EncryptWordpressPassword(string password)
    {
        var randomBytes = this.GetRandomBytes();

        var hash = this.WordpressCrypt(password, this.GenerateSalt(randomBytes));

        return hash;
    }

    private byte[] GetRandomBytes()
    {
        var rnd = new RNGCryptoServiceProvider();
        var b = new byte[6];
        rnd.GetBytes(b);
        return b;
    }

    private string GenerateSalt(byte[] input)
    {
        var output = "$P$";
        output = output + itoa64[11];
        output = output + this.Encode64(input, 6);

        return output;
    }

    private bool WpCheckPassword(string password, string hash)
    {
        var resultHash = this.WordpressCrypt(password, hash);

        return resultHash == hash;
    }

    private string WordpressCrypt(string password, string settings)
    {
        string output = "*0";
        if (settings == null)
        {
            return output;
        }

        if (settings.StartsWith(output))
            output = "*1";

        string id = settings.Substring(0, 3);
        // We use "$P$", phpBB3 uses "$H$" for the same thing
        if (id != "$P$" && id != "$H$")
            return output;

        // get who many times will generate the hash
        int count_log2 = itoa64.IndexOf(settings[3]);
        if (count_log2 < 7 || count_log2 > 30)
            return output;

        int count = 1 << count_log2;

        string salt = settings.Substring(4, 8);
        if (salt.Length != 8)
            return output;

        byte[] hashBytes = { };
        using (MD5 md5Hash = MD5.Create())
        {
            hashBytes = md5Hash.ComputeHash(Encoding.ASCII.GetBytes(salt + password));
            byte[] passBytes = Encoding.ASCII.GetBytes(password);
            do
            {
                hashBytes = md5Hash.ComputeHash(hashBytes.Concat(passBytes).ToArray());
            } while (--count > 0);
        }

        output = settings.Substring(0, 12);
        string newHash = Encode64(hashBytes, 16);

        return output + newHash;
    }

    private string Encode64(byte[] input, int count)
    {
        StringBuilder sb = new StringBuilder();
        int i = 0;
        do
        {
            int value = (int)input[i++];
            sb.Append(itoa64[value & 0x3f]); // to uppercase
